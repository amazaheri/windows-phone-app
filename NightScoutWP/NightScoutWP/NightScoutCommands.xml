<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="commandSet_en-us">
    <CommandPrefix> NightScout, </CommandPrefix>
    <Example> Show latest reading </Example>

    <Command Name="showLatestReading">
      <Example> Show me current bg </Example>
      <ListenFor> [show] [Me] [current] {bgReading}</ListenFor>
      <Feedback> Showing the current {bgReading} reading </Feedback>
      <Navigate Target="MainPage.xaml"/>
    </Command>
    
    <PhraseList Label="bgReading">
      <Item> bg </Item>
      
    </PhraseList>   

  </CommandSet>

  <!-- Other CommandSets for other languages -->

</VoiceCommands>